{
  "name": "fc-injector",
  "version": "1.1.0",
  "description": "Electron app to inject css into fc2",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build:windows": "electron-packager . --asar --platform=win32 --overwrite --out ./bin",
    "build:macos": "electron-packager . --asar --platform=darwin --overwrite --out ./bin",
    "build:linux": "electron-packager . --asar --platform=linux --overwrite --out ./bin",
    "build:all": "npm-run-all --parallel build:windows build:macos build:linux"
  },
  "repository": "https://github.com/fooooooooooooooo/fc-injector",
  "author": "fooooooooooooooo",
  "license": "MIT",
  "devDependencies": {
    "electron-packager": "^15.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prettier-eslint": "^13.0.0"
  },
  "dependencies": {
    "bulma": "^0.9.3",
    "electron": "^13.2.1",
    "monaco-editor": "^0.27.0"
  }
}
