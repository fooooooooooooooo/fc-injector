{
  "name": "fc-injector",
  "version": "1.1.0",
  "description": "Electron app to inject css into fc2",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build:windows_x64": "electron-packager . --asar --platform=win32 --arch=x64 --overwrite --out ./bin",
    "build:windows_x86": "electron-packager . --asar --platform=win32 --arch=ia32 --overwrite --out ./bin",
    "build:macos": "electron-packager . --asar --platform=darwin --overwrite --out ./bin",
    "build:linux_x86": "electron-packager . --asar --platform=linux --arch=x64 --overwrite --out ./bin",
    "build:linux_x64": "electron-packager . --asar --platform=linux --arch=ia32 --overwrite --out ./bin",
    "build:all_x64": "electron-packager . --asar --platform=win32,darwin,linux --arch=x64 --overwrite --out ./bin",
    "build:all_x86": "electron-packager . --asar --platform=win32,linux --arch=ia32 --overwrite --out ./bin",
    "build:all": "yarn build:all_x64 & yarn build:all_x86"
  },
  "repository": "https://github.com/fooooooooooooooo/fc-injector",
  "author": "fooooooooooooooo",
  "license": "MIT",
  "devDependencies": {
    "electron-packager": "^15.3.0",
    "electron": "^13.2.2",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint": "^7.32.0",
    "prettier-eslint": "^13.0.0",
    "prettier": "^2.3.2"
  },
  "dependencies": {
    "bulma": "^0.9.3",
    "monaco-editor": "^0.27.0"
  }
}
