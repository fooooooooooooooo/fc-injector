{
  "name": "fc-injector",
  "version": "2.0.0",
  "description": "Electron app to inject css into fc2",
  "main": "packages/main/dist/index.cjs",
  "repository": "https://github.com/fooooooooooooooo/fc-injector",
  "author": "fooooooooooooooo",
  "license": "MIT",
  "engines": {
    "node": ">=v14.16",
    "npm": ">=7.7"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "build-prod": "cross-env MODE=production yarn build",
    "compile-win32": "electron-builder build --config electron-builder.config.js --win --ia32 --dir --config.asar=true",
    "compile-win64": "electron-builder build --config electron-builder.config.js --win --x64 --dir --config.asar=true",
    "compile-linux32": "electron-builder build --config electron-builder.config.js --l --ia32 --dir --config.asar=true",
    "compile-linux64": "electron-builder build --config electron-builder.config.js --l --x64 --dir --config.asar=true",
    "compile-mac": "electron-builder build --config electron-builder.config.js --mac --dir --config.asar=true",
    "compile-all32": "electron-builder build --config electron-builder.config.js --win --linux --ia32 --dir --config.asar=true",
    "compile-all64": "electron-builder build --config electron-builder.config.js --win --linux --ia32 --dir --config.asar=true",
    "pretest": "cross-env MODE=test yarn build",
    "test": "node tests/app.spec.js",
    "dev": "node scripts/watch.js",
    "lint": "eslint . --ext js,vue"
  },
  "browserslist": [
    "Chrome 91"
  ],
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "2.2.0",
    "@vitejs/plugin-vue": "1.6.1",
    "@vue/compiler-sfc": "^3.2.6",
    "cross-env": "7.0.3",
    "electron": "13.2.2",
    "electron-builder": "22.13.1",
    "electron-devtools-installer": "3.2.0",
    "eslint": "7.32.0",
    "eslint-plugin-vue": "7.17.0",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.1.0",
    "lint-staged": "11.1.2",
    "prettier": "^2.3.2",
    "prettier-eslint": "^13.0.0",
    "sass": "^1.39.0",
    "simple-git-hooks": "2.6.1",
    "spectron": "15.0.0",
    "vite": "2.5.3"
  },
  "dependencies": {
    "@vueuse/core": "^6.0.0",
    "bulma": "^0.9.3",
    "electron-updater": "4.5.0",
    "monaco-editor": "^0.27.0",
    "vue": "3.2.9",
    "vue-router": "4.0.11"
  }
}
